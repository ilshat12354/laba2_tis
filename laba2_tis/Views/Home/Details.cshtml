@model User
@{
 ViewBag.Title = Model.Name;
}
<h2>Пользователь @Model.Name</h2>
<div>
 <dl class="dl-horizontal">
 <dt>Идентификатор</dt>
 <dd>@Model.Id</dd>
 <dt>Электронная почта</dt>
 <dd>@Model.Email</dd>
 <dt>Номер телефона</dt>
 <dd>@Model.Phone</dd>
 <dt>Возраст</dt>
 <dd>@Model.Age</dd>
 </dl>
</div>